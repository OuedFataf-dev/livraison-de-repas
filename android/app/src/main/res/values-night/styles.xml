<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!-- Thème appliqué à la fenêtre Android pendant le démarrage du processus lorsque le paramètre Mode sombre du système d'exploitation est activé -->
    <style name="LaunchTheme" parent="Theme.AppCompat.DayNight.NoActionBar">
        <!-- Afficher un écran de démarrage sur l'activité. Supprimé automatiquement lorsque le moteur Flutter dessine sa première image -->
        <item name="android:windowBackground">@drawable/launch_background</item>
    </style>
    
    <!-- Thème appliqué à la fenêtre Android dès que le processus a démarré. Ce thème détermine la couleur de la fenêtre Android pendant l'initialisation de votre interface utilisateur Flutter, ainsi que derrière votre interface utilisateur Flutter pendant son exécution. -->
    <style name="NormalTheme" parent="Theme.AppCompat.DayNight.NoActionBar">
        <item name="android:windowBackground">?android:colorBackground</item>
    </style>
</resources>
